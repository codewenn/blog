{
	"env": { "node": true, "es6": true },
	"extends": [
		"next/core-web-vitals",
		"prettier",
		"plugin:prettier/recommended"
	],
	"plugins": ["prettier"],
	"parserOptions": {
		"ecmaFeatures": { "jsx": true },
		"ecmaVersion": "latest",
		"sourceType": "module"
	},
	"root": true,
	"rules": {
		"quotes": ["error", "single"],
		"semi": "off",
		"jsx-quotes": "off",
		"prettier/prettier": "error"
	}
}
